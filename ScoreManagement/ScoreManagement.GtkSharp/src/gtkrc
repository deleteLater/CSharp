style "xfdesktop-icon-view" 
{
  ## background colors
  base[NORMAL] 		= @tooltip_bg_color
  fg[NORMAL] 		= @selected_fg_color

  base[ACTIVE] 		= mix(0.70, @selected_bg_color, @selected_fg_color)
  fg[ACTIVE] 		= @selected_fg_color

  base[SELECTED] 	= @selected_bg_color
  fg[SELECTED] 		= @selected_fg_color

  ## whether or not unselected icon text gets truncated (...)
  # XfdesktopIconVIew::ellipsize-icon-labels = 1

  ## text shadow to be painted with the icon labels
  XfdesktopIconView::shadow-x-offset 			= 1
  XfdesktopIconView::shadow-y-offset 			= 1
  XfdesktopIconView::shadow-color 				= shade(0.50, @selected_fg_color)
  XfdesktopIconView::selected-shadow-x-offset 	= 1
  XfdesktopIconView::selected-shadow-y-offset 	= 1
  XfdesktopIconView::selected-shadow-color 		= shade(0.50, @selected_fg_color)
  
  ## opacity of text background (0 - 255, 0 = transparent)
  XfdesktopIconView::label-alpha 				= 210
  XfdesktopIconView::selected-label-alpha 		= 210
  XfdesktopIconVIew::ellipsize-icon-labels 		= 1

  ## spacing and sizing of icons on the grid
  #XfdesktopIconVIew::cell-spacing = 6
  #XfdesktopIconView::cell-padding = 2
  XfdesktopIconView::cell-text-width-proportion = 4.0

        engine "murrine" {
        #############
        textstyle = 0
        #############
		animation			= TRUE
		arrowstyle			= 1	# Estilo flechas -> triangulos
		border_shades       = {0.92, 0.92}
		colorize_scrollbar	= FALSE
		contrast			= 0.20
        focus_color         = @text-prelight
		default_button_color = "#cccccc"
		glowstyle			= 4 # 0 = top, 1 = bottom, 2 = top and bottom, 3 = horizontal, 4 = centered glow
        glow_shade          = 1.02
		gradient_shades		= {1.00, 0.99, 0.99, 1.00}
		highlight_shade		= 1.0
		lightborderstyle	= 1 # 0 = top side, 1 = all sides
		lightborder_shade	= 1.0
        menubaritemstyle    = 0 #menuitem look
        prelight_shade      = 1.0
        progressbarstyle    = 0 # 0 = no stripes
        reliefstyle         = 0 # flat
        rgba                = TRUE #rgba enabled
		roundness			= 0
        separatorstyle      = 1 # smooth separator
		shadow_shades		= {1.0, 1.0}
        trough_border_shades = { 0.90, 0.90 }
		trough_shades		= { 1.10, 1.10 }
		}
}
widget_class "*XfdesktopIconView*" 			style "xfdesktop-icon-view"

