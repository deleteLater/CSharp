	xthickness = 1
	ythickness = 1


	bg[NORMAL] 			= @bg_color
	bg[PRELIGHT] 		= @bg_color
	bg[ACTIVE] 			= @selected_bg_color
	bg[SELECTED] 		= @selected_bg_color
	bg[INSENSITIVE] 	= @bg_color

	fg[NORMAL] 		    = @text_color
	fg[PRELIGHT] 	    = @text-prelight
	fg[ACTIVE] 		    = @text_color
	fg[SELECTED] 		= @selected_fg_color
	fg[INSENSITIVE] 	= shade(0.60 ,@selected_fg_color)

	base[NORMAL] 		= @base_color
	base[PRELIGHT] 		= mix(0.50, @selected_bg_color, @selected_fg_color)
    base[ACTIVE]        = mix(0.80, @selected_bg_color, @selected_fg_color)
	base[SELECTED]		= @selected_bg_color
	base[INSENSITIVE] 	= @bg_color

	text[NORMAL] 		= @text_color
	text[PRELIGHT] 		= @text-prelight
	text[ACTIVE] 		= @selected_fg_color
	text[SELECTED] 		= @selected_fg_color
	text[INSENSITIVE] 	= shade(0.60 ,@selected_fg_color)


