style "notebook_spinbutton"
{
	xthickness = 5
	ythickness = 5

			# Normal:
            base[NORMAL] 		= @selected_fg_color
            text[NORMAL]    	= @text_color

			# Selected:
            base[SELECTED]	    = @selected_bg_color
            text[SELECTED]	    = @selected_fg_color

			# Active:
			base[ACTIVE]	    = @selected_fg_color
			text[ACTIVE]	    = @fg_color

			# insensitive:
			base[INSENSITIVE]   = "#f0f0f0"
            text[INSENSITIVE] 	= shade(0.60 ,@selected_fg_color)


            # buttons:
            bg[NORMAL]      = "#cccccc"
            bg[PRELIGHT]    = "#cccccc"
            bg[ACTIVE]      = "#cccccc"
            bg[INSENSITIVE] = "#dcdee0"

            # Progress-Bar
            bg[SELECTED]	= @selected_bg_color    # Progress-Bar

            # arrows:
            fg[NORMAL]          = "black"
            fg[PRELIGHT]        = "white"
            fg[ACTIVE]          = "red"
            fg[INSENSITIVE]     = "#b4b4b4"


	engine "pixmap"
	{
    # Buttons
		image
		{
		function			= BOX
		detail				= "spinbutton_up"
		recolorable			= TRUE
		file				= "SpinButton/spin_up.svg"
		border				= { 1,1,1,1 }
		stretch				= TRUE
		}

		image
		{
		function			= BOX
		detail				= "spinbutton_down"
		recolorable			= TRUE
		file				= "SpinButton/spin_down.svg"
		border				= { 1,1,1,1 }
		stretch				= TRUE
		}


    # Arrows
        image
        {
        function		    = ARROW
        state               = NORMAL
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = PRELIGHT
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up-prelight.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = ACTIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up-active.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = INSENSITIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up-insensitive.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = NORMAL
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }

        image
        {
        function		    = ARROW
        state			    = PRELIGHT
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down-prelight.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }

        image
        {
        function		    = ARROW
        state			    = ACTIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down-active.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }

        image
        {
        function		    = ARROW
        state			    = INSENSITIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down-insensitive.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }


	# SPIN ENTRY ##############
		image
		{
		function			= FOCUS
		recolorable			= TRUE
		file				= "Entry/notebook-entry-active.svg"
		border				= { 1,1,1,1 }
		stretch				= TRUE
		}	
		image
		{
		function			= SHADOW
		detail				= "entry"
		shadow				= IN
		recolorable			= FALSE
		file				= "Entry/notebook-entry.svg"
		border				= { 1,1,1,1 }
		stretch				= TRUE
		}
	}
