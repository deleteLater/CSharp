            image
            {
            function			= HANDLE
            state               = NORMAL
            recolorable			= TRUE
            file				= "Assets/null.png"
            overlay_file		= "Assets/null.png"
            overlay_border		= {0,1,1,1 }
            overlay_stretch		= FALSE
            orientation			= VERTICAL
            }
            
            image
