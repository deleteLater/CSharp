gtk_color_scheme = "base_color:#F8F8F8\nbg_color:#e6e8eb"
gtk_color_scheme = "fg_color:#383838\ntext_color:#000000"
gtk_color_scheme = "selected_bg_color:#0085CA\nselected_fg_color:#FFFFFF"
gtk-color-scheme = "tooltip_bg_color:#656D78\ntooltip_fg_color:#FFFFFF"
gtk_color_scheme = "redcolor:#cd3333\nbluecolor:#97FFFF"
gtk_color_scheme = "text-prelight:#0085CA\nlink-color:#C200AB"
gtk_color_scheme = "notebook_bg:#ecf0f1"

gtk-auto-mnemonics		= 1 # Disables ugly lines under menu items
gtk-enable-animations 	= 1  
gtk-tooltip-timeout 	= 1200
gtk-menu-popup-delay 	= 360


