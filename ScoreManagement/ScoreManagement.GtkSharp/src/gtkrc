            fg[ACTIVE]          = "red"
            fg[INSENSITIVE]     = "#b4b4b4"

    engine "pixmap" 
    {
            # LTR & RTL

			image 
			{
			function		= BOX
			state    		= NORMAL
			file			= "Buttons/button-normal.svg"		
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}	

			image
			{
			function		= BOX
			state			= PRELIGHT
			recolorable		= TRUE
			file			= "Buttons/button-prelight.svg"
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}

			image
			{
			function		= BOX
			state			= ACTIVE
			file			= "Buttons/button-active.svg"
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}

			image 
			{
			function		= BOX
			state			= INSENSITIVE
			file			= "Buttons/button-insensitive.svg"
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}
    }
}
