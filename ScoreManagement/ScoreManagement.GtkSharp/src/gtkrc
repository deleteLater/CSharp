style "gimp-display" = "scrollbar" 
{
    GimpRuler::font-scale          = 0.6444
    GimpUnitComboBox::label-scale  = 0.8333
    GimpScaleComboBox::label-scale = 0.8333
    GtkComboBox::arrow-size        = 8
    GtkButton::inner-border        = { 0,0,0,0 }
    GtkButton::focus-line-width    = 0
    GtkButton::focus-padding       = 0

	bg_pixmap[NORMAL]   = "Assets/bg.svg"
	bg[NORMAL]          = @bg_color

	fg[NORMAL] 		    = @fg_color

    engine "pixmap" 
    {
            ########### TROUGH
            image 
            {
            function		= BOX
            detail			= "trough"
            file			= "Scrollbars/trough-scrollbar-horizontal.svg"
            border			= { 1,1,1,1 }
            stretch			= TRUE
            orientation		= HORIZONTAL
            }
            image 
            {
            function		= BOX
            detail			= "trough"
            file			= "Scrollbars/trough-scrollbar-vertical.svg"
            border			= { 1,1,1,1 }
            stretch			= TRUE
            orientation		= VERTICAL
            }

            ########### SLIDERS HORIZONTAL
            image 
            {
            function			= SLIDER
            state           	= NORMAL
            file				= "Scrollbars/slider-horizontal.svg" 
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= HORIZONTAL
            }
            image 
            {
            function			= SLIDER
            state           	= PRELIGHT
            file				= "Scrollbars/slider-horizontal-prelight.svg" 
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= HORIZONTAL
            }
            image 
            {
            function			= SLIDER
            state           	= ACTIVE
            file				= "Scrollbars/slider-horizontal-active.svg" 
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= HORIZONTAL
            }
            image 
            {
            function			= SLIDER
            state           	= INSENSITIVE
            file				= "Scrollbars/slider-horizontal-insensitive.svg"
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= HORIZONTAL
            }

            ########### SLIDERS VERTICAL
            image 
            {
            function			= SLIDER
            state           	= NORMAL
            file				= "Scrollbars/slider-vertical.svg" 
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= VERTICAL
            }
            image 
            {
            function			= SLIDER
            state           	= PRELIGHT
            file				= "Scrollbars/slider-vertical-prelight.svg" 
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= VERTICAL
            }
            image 
            {
            function			= SLIDER
            state           	= ACTIVE
            file				= "Scrollbars/slider-vertical-active.svg" 
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= VERTICAL
            }
            image 
            {
            function			= SLIDER
            state           	= INSENSITIVE
            file				= "Scrollbars/slider-vertical-insensitive.svg"
            border				= { 1,1,1,1 }
            stretch				= TRUE
            orientation			= VERTICAL
            }

            ########### STEPPERS
			image 
			{
			function				= BOX
            file				    = "Scrollbars/stepper.svg"
            border				    = { 0,0,0,0 }
			stretch					= TRUE
			}
			
            ########### ARROWS
            image
            {
            function		    = ARROW
            state               = NORMAL
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-normal.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }

            image
            {
            function		    = ARROW
            state			    = PRELIGHT
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-prelight.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }

            image
            {
            function		    = ARROW
            state			    = ACTIVE
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-active.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }

            image
            {
            function		    = ARROW
            state			    = INSENSITIVE
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-insensitive.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }
    }
