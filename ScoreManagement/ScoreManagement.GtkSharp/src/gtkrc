        fg[PRELIGHT]	= @selected_fg_color
		fg[ACTIVE]      = @text_color
		fg[INSENSITIVE] = shade(0.60, @selected_fg_color)


    engine "pixmap"
    {
			image 
			{
			function		= BOX
			state    		= NORMAL
			file			= "Buttons/toolbar-button-normal.svg"		
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}	

			image
			{
			function		= BOX
			state			= PRELIGHT
			recolorable		= TRUE
			file			= "Buttons/logout.svg"
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}

			image
			{
			function		= BOX
			state			= ACTIVE
			file			= "Buttons/button-active.svg"
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}

			image 
			{
			function		= BOX
			state			= INSENSITIVE
			file			= "Buttons/button-insensitive.svg"
			border			= { 2,2,2,2 }
			stretch			= TRUE
			}
	}
}
